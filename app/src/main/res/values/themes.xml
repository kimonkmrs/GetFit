<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.MyApp2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Base application theme. -->


        <!-- Primary brand color. -->


        <item name="colorPrimary" type="color" tools:ignore="MissingDefaultResource">@color/purple_500</item>
        <item name="colorPrimaryVariant" type="color">@color/purple_700</item>
        <item name="colorOnPrimary" type="color">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary" type="color">@color/teal_200</item>
        <item name="colorSecondaryVariant" type="color">@color/teal_700</item>
        <item name="colorOnSecondary" type="color">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:ignore="ObsoleteSdkInt" tools:targetApi="21" type="color">?attr/colorPrimaryVariant</item>


    </style>

</resources>